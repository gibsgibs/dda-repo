@using Microsoft.AspNetCore.Authorization
@inject IAuthorizationService AuthorizationService
@model string

@if ((await AuthorizationService.AuthorizeAsync(User, "SuperAdminAndHigher")).Succeeded)
{
    <div class="card-footer d-flex">
        <a class="btn btn-primary ml-auto" asp-page="/Admin/SuperAdmin" asp-route-id="@Model">SuperAdmin Edit</a>
    </div>
}
